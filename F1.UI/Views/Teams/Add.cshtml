@using F1.UI.Models.Domains
@model AddTeamViewModel
@{
    <h3>Add Team</h3>

    <form method="post">
        <div class="mt-3">
            <label class="form-label">Name</label>
            <input type="text" class="form-control" asp-for="Name"/>
        </div>

        <div class="mt-3">
            <button type="submit" class="btn btn-success">Save</button>
        </div>
    </form>
}

